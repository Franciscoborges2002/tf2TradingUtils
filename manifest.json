{
  "manifest_version": 3,

  "name": "tf2TradingUtils",
  "version": "1.0.0",

  "description": "Some scripts for tf2 trading",
  "author": "Francisco Borges",

  "content_scripts": [
    {
      "js": ["./scrap.tf/scrapItemLinks/content.js"],
      "matches": ["https://scrap.tf/items*"]
    },
    {
      "js": ["./stntrading.eu/unusualBackpack/content.js"],
      "matches": ["https://stntrading.eu/item/tf2/Unusual*"]
    },
    {
      "js": ["./stntrading.eu/copyClipboard/content.js"],
      "matches": ["https://stntrading.eu/item*"]
    },
    {
      "js": ["./backpack.tf/oldUI/addKSButton/content.js"],
      "matches": ["https://backpack.tf/stats/*"]
    },
    {
      "js": ["./steamcommunity.com/steamLinks/content.js"],
      "matches": [
        "https://steamcommunity.com/profiles/*",
        "https://steamcommunity.com/id/*"
      ]
    },
    {
      "js": ["./steamcommunity.com/steamTradeHelper/content.js"],
      "matches": ["https://steamcommunity.com/tradeoffer/new/*"]
    }
  ],
  "icons": {
    "256": "public/icons/png/icon_256.png",
    "128": "public/icons/png/icon_128.png",
    "64": "public/icons/png/icon_64.png",
    "48": "public/icons/png/icon_48.png",
    "32": "public/icons/png/icon_32.png",
    "16": "public/icons/png/icon_16.png"
  }
}
